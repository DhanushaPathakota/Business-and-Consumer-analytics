<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>Login Page </title>


<style>

* {
			box-sizing: border-box;
	}

	body {
			margin: 30px;

	}

	.confirmation
	{
		width: 700px;
		margin: 0 auto;
		font-family: Tahoma, Geneva, sans-serif;

border-radius: 5px;
background-color: #BFC9CA;
padding: 50px;
	}

	h1{
		color:#154360
	}

	.conf
	{
		width: 600px;
		margin: 0 auto;
		font-family: Tahoma, Geneva, sans-serif;
		border-radius: 5px;
		background-color: #D6DBDF;
		padding: 50px;
	}

</style>
</head>
  <body>
    <div class="confirmation">
    <h1><center>New Service Requests:</center></h1>
    <div class="conf">
    <%list.forEach(function(it){%>
      <h4>Company Name:<%=it.cname%></h4>

      <h4>email  :  <%=it.email%></h4>
      <h4>phone number :  <%=it.phno%></h4>
      <h4>Url:  <%=it.url%></h4>
      <h4>subject:<%=it.subject%></h4>
      <h4>description:<%=it.details%></h4>
      <h4>Date:<%=it.reg_date%></h4><br><br>
      <%});%>
  <button onclick="location.href = '/adminconsole';" id="myButton" class="float-left submit-button" >Back to previous page</button>
</div>
</div>
    </body>
    </html>